*.background:		#222222
*.foreground:		#FFFFFF
*.cursorColor:		#DDDDDD

URxvt.depth:		24
URxvt.font: xft:MesloLGS Nerd Font:pixelsize=12,xft:Noto Color Emoji:pixelsize=12
URxvt.transparent:	true
URxvt.shading:		20
URxvt.scrollColor:	#FF00FF
URxvt.scrollBar:	false
URxvt.scrollBar_right:	false

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select:resite-font
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true

URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.keysym.C-minus:		resize-font:smaller
URxvt.keysym.C-plus:		resize-font:bigger
URxvt.keysym.C-equals:		resize-font:reset
URxvt.keysym.C-question:	resize-font:show
